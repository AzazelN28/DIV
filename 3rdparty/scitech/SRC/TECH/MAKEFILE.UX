include $(SCITECH)/makedefs/startup.mk

# Name of library and object files required to build it

ifdef CHECKS
CFLAGS          += -DDEBUG=2
LIBFILE         = $(LP)techd$L
else
ifdef STKCALL
LIBFILE         = $(LP)stech$L
else
LIBFILE         = $(LP)tech$L
endif
endif
OBJECTS         = dlist$O list$O hashtab$O error$O options$O str$O \
				  dynstr$O substr$O memmgr$O hash_c$O slist$O

# Define the list of object files to create dependency information for

DEPEND_OBJ      = $(OBJECTS) hashtst$O tststr$O

include $(SCITECH)/makedefs/common.mk

